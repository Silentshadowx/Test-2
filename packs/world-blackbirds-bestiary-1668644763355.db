>\n<p><span class=\"fontstyle0\">&nbsp;</span><span class=\"fontstyle2\">(Arduous &ndash;30%): </span><span class=\"fontstyle3\">Fire at a foe in the dark</span> </p>\n</section>","requiresTraining":true,"associatedPrimaryAttribute":"combat"},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"3Qb5XdgRqXJnjbNX":3},"flags":{"scene-packer":{"hash":"0c39a273d8fb8750a1a85184f6e0dc9080f9d35e","sourceId":"Compendium.world.blackbirdsskills.IMnVwayiLjG9jMAs"},"core":{"sourceId":"Item.svJ9P7E4SoGwJwb2"}}},{"_id":"iMZBbSTTHSZ91b2F","name":"Education","type":"skill","img":"systems/zweihander/assets/icons/skills.svg","data":{"description":{"@en":""},"notes":"","requiresTraining":true,"associatedPrimaryAttribute":"Intelligence"},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"3Qb5XdgRqXJnjbNX":3},"flags":{"core":{"sourceId":"Compendium.zweihander.skills.RUgfMXCfP6bMvRe8"},"scene-packer":{"sourceId":"Compendium.world.blackbirdsskills.UhDV77vmuQGFnSzW","hash":"e4e029c47b462b21ace47cdf5d196c2c2c48e2ed"}}},{"_id":"iNae45GB9AhudON3","name":"Eavesdrop","type":"skill","img":"systems/zweihander/assets/icons/skills.svg","data":{"description":{"@en":""},"notes":"","requiresTraining":false,"associatedPrimaryAttribute":"perception"},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"3Qb5XdgRqXJnjbNX":3},"flags":{"core":{"sourceId":"Compendium.zweihander.skills.y5hb3qmMF41dAFlL"},"scene-packer":{"sourceId":"Compendium.world.blackbirdsskills.opOdtFuyzurnbf0I","hash":"1b10ba9006ad56e694a66fa3c4be8a05955abc96"}}},{"_id":"kX7XL4D3FpzdvHV4","name":"Charm","type":"skill","img":"systems/zweihander/assets/icons/skills.svg","data":{"description":{"@en":""},"notes":"","requiresTraining":false,"associatedPrimaryAttribute":"Fellowship"},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"3Qb5XdgRqXJnjbNX":3},"flags":{"core":{"sourceId":"Compendium.zweihander.skills.zqsz1qIreYXNy56A"},"scene-packer":{"sourceId":"Compendium.world.blackbirdsskills.hubrYvWPjE